<h1>Log in</h1>
<div id='user' data
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash#{name}" %>
<% end %>
<%= form_tag "log_in" do |f| %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <%= submit_tag "Log in" %>
<% end %>
